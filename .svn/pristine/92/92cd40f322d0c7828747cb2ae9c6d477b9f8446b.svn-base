#import "QuickTransactionAlert.h"

@implementation QuickTransactionAlert

- (instancetype)initQuickTransactionAlert
{
   
    self = [super init];
    if (self) {
        
        [self setupAutoHeightWithBottomView:self.canBtn bottomMargin:10];
        self.type = LxCustomAlertTypeSheet;
        
        self.maskView.alpha = 1;
        
    }
    return self;
        

    
}

